{
  "private": true,
  "name": "giphy",
  "version": "1.0.0",
  "description": "An application to search for cool GIFs.",
  "main": "rollup.config.js",
  "scripts": {
    "build": "rollup --config --environment NODE_ENV:production",
    "deploy": "npm run build && gh-pages -d public",
    "start": "rollup --config --watch",
    "test": "stylelint **/*.scss && semistandard --verbose | snazzy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amin-Mohamed/GIPHY.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Amin-Mohamed/GIPHY/issues"
  },
  "homepage": "https://github.com/Amin-Mohamed/GIPHY#readme",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "cssnano": "^4.1.10",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.13.1",
    "postcss-normalize": "^7.0.1",
    "query-string": "^6.2.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-browsersync": "^1.0.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-inject-env": "^0.1.0",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-postcss": "^2.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "semistandard": "^13.0.1",
    "snazzy": "^8.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.1"
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": "stylelint-config-standard"
  },
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ]
}
